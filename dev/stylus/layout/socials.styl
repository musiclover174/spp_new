.socials
  df(center, flex-start)
  @media $LandscapeMedium
    dn()
  &__href
    transition .7s background
    & + &
      ml 30px
    &[data-type="fb"]
      size 7px 14px
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="tw"]
      size 13px 12px
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="yt"]
      size 14px 10px
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="od"]
      size 7px 13px
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')
    &[data-type="vk"]
      size 15px 8px
      background url('data:image/png;base64,') no-repeat center
      &:hover
        background-image url('data:image/png;base64,')